# Docker
# core rule

@if @not($first_run) {
    domain ip table nat {
        chain DOCKER @preserve;
        chain DOCKER-INGRESS @preserve;
    }

    domain ip table filter {
        chain DOCKER @preserve;
        chain DOCKER-INGRESS @preserve;
        chain DOCKER-ISOLATION-STAGE-1 @preserve;
        chain DOCKER-ISOLATION-STAGE-2 @preserve;
        chain DOCKER-USER @preserve;
    }
}
